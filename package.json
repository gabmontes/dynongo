{
	"name": "dynongo",
	"version": "0.19.0",
	"description": "MongoDB like syntax for DynamoDB",
	"license": "MIT",
	"repository": "SamVerschueren/dynongo",
	"author": {
		"name": "Sam Verschueren",
		"email": "sam.verschueren@gmail.com",
		"url": "github.com/SamVerschueren"
	},
	"maintainers": [
		{
			"name": "Gonzalo D'Elia",
			"email": "gonzalo.n.delia@gmail.com",
			"url": "https://github.com/gndelia"
		}
	],
	"main": "dist/index.js",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"clean": "del-cli dist",
		"compile": "npm run clean && tsc",
		"lint": "tslint --format stylish --project .",
		"prepublish": "npm run clean && npm run compile -- --declaration && del-cli dist/test",
		"pretest": "npm run compile -- --sourceMap && cpy \"test/**/*.json\" ../dist --cwd=src --parents",
		"test": "echo 'Tests are disabled until they are fixed due to AWS-SDK v3 migration!"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"dynamodb",
		"mongodb",
		"mongo",
		"mongoose",
		"aws",
		"database",
		"nosql"
	],
	"dependencies": {
		"@aws-sdk/client-dynamodb": "3.362.0",
		"@aws-sdk/lib-dynamodb": "3.362.0",
		"@aws-sdk/types": "3.468.0",
		"@aws-sdk/util-dynamodb": "3.362.0",
		"@smithy/node-http-handler": "2.2.1",
		"delay": "4.1.0",
		"is-object": "1.0.1",
		"p-retry": "4.1.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.2.0",
		"@types/node": "^10.12.21",
		"@types/sinon": "^7.5.0",
		"ava": "^1.4.1",
		"codecov": "^3.1.0",
		"cpy-cli": "^2.0.0",
		"del-cli": "^1.1.0",
		"load-json-file": "^5.1.0",
		"nyc": "^15.1.0",
		"sinon": "^7.5.0",
		"tslint": "^5.12.1",
		"typescript": "3.9.10"
	},
	"typings": "dist",
	"nyc": {
		"exclude": [
			"dist/test"
		]
	}
}
